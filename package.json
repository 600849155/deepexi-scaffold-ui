{
  "name": "deepexi-scaffold-ui",
  "version": "0.3.0",
  "description": "DeepEXI 脚手架 UI 可视化操作界面",
  "dependencies": {
    "commander": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "husky": "^2.7.0",
    "standard-version": "^6.0.1"
  },
  "scripts": {
    "start": "cd back && npm run start",
    "stop": "cd back && npm run stop",
    "dev": "cd back && npm run dev",
    "test": "cd back && npm run test",
    "init": "cd fronted && rm -rf ../back/app/public && npm i && npm run build && cd ../back && npm i -production",
    "release": "standard-version"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepexi/deepexi-scaffold-ui.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bin": {
    "scaffold-ui": "bin/scaffold-ui.js"
  },
  "author": "lyTongXue",
  "license": "MIT"
}
